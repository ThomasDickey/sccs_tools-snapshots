# make-file for testing fix-sccs utility


@make_include_left@@TD_LIB_rules@/td_lib.mk@make_include_right@

####### (Standard Lists) #######################################################
SCRIPTS	=\
	run_test.sh

SOURCES	= $(SCRIPTS)

####### (Standard Productions) #################################################
all:	$(SOURCES) $(ALL)
clean:				; $(RM) $(CLEAN)
distclean:	clean		; -$(RM) $(ALL)
check:		$(SOURCES)	; $(RUN_TESTS)
lint.out:

####### (Details of Productions) ###############################################
$(SOURCES):
